{
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "family": "LearnJenkinsApp-TaskDefinition-Prod",
  "containerDefinitions": [
    {
      "name": "learnjenkinsapp",
      "image": "nginx:1.27-alpine",
      "portMappings": [
        {
          "name": "nginx-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "0.5 GB",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::748972035298:role/ecsTaskExecutionRole"
}